  # VPN SERVICES AND PORTS

- VMESS-WS-TLS           : 443 
- VMESS-WS-NONTLS        : 80 
- VLESSWS-TLS            : 443
- VLESS-WS-NONTLS        : 8000
- TROJAN TCP             : 443
- VLESS-XTLS-DIRECT      : 443
- VLESS-XTLS-SPLICE      : 443

- NGINX                  : 81


  


  


